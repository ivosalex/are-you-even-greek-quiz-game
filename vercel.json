{
  "buildCommand": "react-scripts build",
  "outputDirectory": "build",
  "headers": [
    {
      "source": "/favicon.:ext",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/favicon.ico", "destination": "/favicon.svg" },
    { "source": "/((?!api/).*)", "destination": "/index.html" }
  ]
}
